#!/bin/csh
# source this file
#
# QQ uses two decay files:  QQ_DECAY_FILE and QQ_USER_FILE
# QQ_DECAY_FILE is defined by the setup proceedure as $QQ_DIR/decay.dec
# before running QQ examples, define the user decay file:
# setenv QQ_USER_FILE user.dec
# Also, isajet uses ISAJET_DECAY to point to it's input decay file.
# When running qq_isajet_exam, you need to redefine ISAJET_DECAY as below.

 setenv QQ_USER_FILE $EVGEN_DIR/input/user.dec
 setenv ISAJET_DECAY $EVGEN_DIR/input/isa_nobc_decay.dat

# These are the basic examples
# to run them, type :
# 
# $MCFBIN/pythia_exam -f pythia_exam.pyt
# $MCFBIN/pythia_exam -f qq_pythia_exam.pyt
# $MCFBIN/herwig_exam -f herwig_exam.hwg
# $MCFBIN/herwig_exam -f qq_herwig_exam.hwg
# $MCFBIN/isajet_exam -l isajet_exam.lpt -o isajet_exam.evt -hb isajet_exam.rz
# $MCFBIN/isajet_exam -qq -l qq_isajet_exam.lpt -o qq_isajet_exam.evt -hb qq_isajet_exam.rz
# 

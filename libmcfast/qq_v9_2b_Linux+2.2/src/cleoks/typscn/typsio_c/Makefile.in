MAKE = gmake
SHELL = /bin/sh
srcdir = @srcdir@
VPATH = @srcdir@
MACHINE = @MACHINE@

CPP = @CPP@
ETAGS = etags -a
MAKEDEPEND = @MAKEDEPEND@
CC = @CC@
CFLAGS = @CFLAGS@
ifeq "$(srcdir)" "."
  absrcdir = $(shell pwd)
else
  absrcdir = $(srcdir)
endif

OBJS=

C_SOURCES=$(wildcard $(srcdir)/*.c)

NOTDIR_C_SOURCES=$(notdir $(C_SOURCES))

OBJS+=$(NOTDIR_C_SOURCES:%.c=$(LIBRARY)(%.o))

LIBRARY=dummy
ifeq "$(LIBRARY)" "dummy"
  OBJS=
endif
.SUFFIXES: .cdf

.PHONY:dummy production vaxsrc vaxcom debug clean distclean realclean TAGS depend fort_depend c_depend cc_depend
	@

dummy::
	@echo Run Makefile in the parent directory

production:$(LIBRARY)
	@ 

vaxsrc:$(VAX_FORTRAN_SOURCES)
	@ 

vaxcom:$(VAX_COMS)
	@ 

debug:$(LIBRARY)
	@ 

clean:
	@rm -f *.o *.bak

distclean:clean
	@rm -f Makefile Makefile.obj

realclean:
	@rm -f *~ #*

$(LIBRARY):$(OBJS)
	@test "$(LIBRARY)" = "dummy" || ar ts $(LIBRARY)

TAGS:$(absrcdir)/*[Fch]
	@$(ETAGS) -f $(dir $(LIBRARY))/TAGS.$(LIBRARY_NAME) $(absrcdir)/*[Fch]

depend:c_depend 
	@test "$(LIBRARY)" != "dummy" || echo Run Makefile in the parent directory

.c.a:
	$(CC) $(CFLAGS) $(CLEO_DEFS) $(CLEO_FFLAGS) -c $<
	@$(AR) r $@ $%
	@$(RM) $%

c_depend:$(C_SOURCES)
	@test "$(LIBRARY)" = "dummy" || test -z "$(C_SOURCES)" || $(MAKEDEPEND) -o.o\) -p$$\(LIBRARY\)\( $(CFLAGS) $(CLEO_DEFS) $(CLEO_FFLAGS) $(C_SOURCES)





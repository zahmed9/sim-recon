!                  7 May 1999
!
!                  The coordinate system has z along the photon beam, y
!                  is defined as vertically up, and x makes the system
!                  right handed.
!
!                  The origin is on the beam axis at the center of the
!                  up stream mirror plate.
!
! ***********  Define the detector as central.
!
include db/detector.db
!
make detector "HALLD STAGE 1.0" "CENTRAL"
!
! ************ Define New Materials.
!
include db/materials.db
!
! ***  CF4 = Freon for Chamber gas
make MATERIAL "CF4"  88.  42.  3.9286E-3 64000. 5.0E+04 1.0E+16 6.75E-06
!
! ************ Define New Mixtures 
!
include db/mixtures.db
!
!
! *** Chamber Gas 
make MIXTURE  "AR-ETH-CF4" 3 "ARGON"  "C2H6"  "CF4"  "-"  "-" 0.5 0.35 0.15
! *** Chamber Gas with including Al. wire.
make MIXTURE  "GAS-MX" 3 "ARGON"  "C2H6"  "CF4"  "ALUM" "-" 0.500 0.35 0.15 
! *** Lead - Scintillator Sandwich for Calorimeter
make MIXTURE  "PBSC"       2 "LEAD"  "SCIN"  "-"  "-"  "-" 0.4 0.6 

!
! ************** Create the Beam pipe
!
include db/beampipe.db
!
! ***.05 cm thick Be pipe at a radius of 2.50cm.
!           name  rmin rmax   z0  zlen   fill  rmin rmax front back  fill  wall  front   back
make BPipe "beam" 0.00 2.50 232.5 465.0 "VACU" 0.00 0.05 0.00  0.00 "VACU" "BE"  "VACU" "VACU"
!
! *************** The LASS Solenoid 
! *** 2.24 T field 
! *** 415cm long!!!!!!!!!!!!!!!
! *** 95cm.
!
include db/solenoid.db
!  
!               name  bfield  rmin      rmax       z0    zlen     FILL  rmin   rmax   zmin    zmax     fill    wall    front   back
make Solenoid  "LASS" 2.24000 0.0000   95.0000    207.5  415.0000 "AIR" 0.0000 2.0000 0.0000  0.0000  "----"  "MXSO"  "----"  "----"
!
!
!                    Barrel Calorimeter
!
include db/emcal.db
make EMCal "BCAL" "TUBE" 1 65 65 90 90 207.5 400 "PBSC" "PBSC" 48 1 0.06 0.01 0.3 0.3 4 1 
!
!       LGD
! LGD is hard coded into HDFast. See usr_lgd.c

<?xml version="1.0" encoding="iso-8859-1" standalone="no" ?>
<HDDM class="s" version="1.0" xmlns:hddm="http://www.gluex.org/hddm" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <physicsEvent eventNo="int" maxOccurs="unbounded" runNo="int">
    <reaction maxOccurs="unbounded" minOccurs="0" type="int" weight="float">
      <beam minOccurs="0" type="Particle_t">
        <momentum E="float" px="float" py="float" pz="float"/>
        <properties charge="int" mass="float"/>
      </beam>
      <target minOccurs="0" type="Particle_t">
        <momentum E="float" px="float" py="float" pz="float"/>
        <properties charge="int" mass="float"/>
      </target>
      <vertex maxOccurs="unbounded">
        <product decayVertex="int" maxOccurs="unbounded" type="Particle_t">
          <momentum E="float" px="float" py="float" pz="float"/>
          <properties charge="int" mass="float"/>
        </product>
        <origin t="float" vx="float" vy="float" vz="float"/>
      </vertex>
    </reaction>
    <hitView version="1.0">
      <centralDC minOccurs="0">
        <cathodeCyl maxOccurs="unbounded" minOccurs="0" radius="float">
          <band maxOccurs="unbounded" phim="float" z="float"/>
        </cathodeCyl>
        <ring maxOccurs="unbounded" minOccurs="0" radius="float">
          <straw maxOccurs="unbounded" phim="float">
            <hit dE="float" maxOccurs="unbounded" minOccurs="0" t="float"/>
            <cdcPoint dEdx="float" dradius="float" maxOccurs="unbounded" minOccurs="0" phi="float" r="float" track="int" z="float"/>
          </straw>
        </ring>
      </centralDC>
      <forwardDC minOccurs="0">
        <chamber layer="int" maxOccurs="unbounded" module="int">
          <cathodePlane maxOccurs="unbounded" minOccurs="0" tau="float" z="float">
            <strip maxOccurs="unbounded" u="float"/>
          </cathodePlane>
          <anodePlane maxOccurs="unbounded" minOccurs="0" tau="float" z="float">
            <wire maxOccurs="unbounded" u="float">
              <hit dE="float" maxOccurs="unbounded" minOccurs="0" t="float"/>
              <fdcPoint dEdx="float" dradius="float" maxOccurs="unbounded" minOccurs="0" track="int" x="float" y="float" z="float"/>
            </wire>
          </anodePlane>
        </chamber>
      </forwardDC>
      <startCntr minOccurs="0">
        <paddle maxOccurs="unbounded" minOccurs="0" phim="float">
          <hit dE="float" maxOccurs="unbounded" minOccurs="0" t="float"/>
        </paddle>
        <startPoint dEdx="float" maxOccurs="unbounded" minOccurs="0" phi="float" r="float" t="float" track="int" z="float"/>
      </startCntr>
      <barrelEMcal minOccurs="0">
        <module maxOccurs="unbounded" minOccurs="0" phim="float">
          <upstream minOccurs="0">
            <shower E="float" maxOccurs="unbounded" t="float"/>
          </upstream>
          <downstream minOccurs="0">
            <shower E="float" maxOccurs="unbounded" t="float"/>
          </downstream>
        </module>
        <barrelShower E="float" maxOccurs="unbounded" minOccurs="0" phi="float" t="float" track="int" z="float"/>
      </barrelEMcal>
      <Cerenkov minOccurs="0">
        <section maxOccurs="unbounded" phim="float">
          <flash maxOccurs="unbounded" pe="float" t="float"/>
        </section>
      </Cerenkov>
      <forwardTOF minOccurs="0">
        <slab maxOccurs="unbounded" minOccurs="0" y="float">
          <left minOccurs="0">
            <hit dE="float" maxOccurs="unbounded" t="float"/>
          </left>
          <right minOccurs="0">
            <hit dE="float" maxOccurs="unbounded" t="float"/>
          </right>
        </slab>
        <tofPoint maxOccurs="unbounded" minOccurs="0" t="float" track="int" x="float" y="float"/>
      </forwardTOF>
      <forwardEMcal minOccurs="0">
        <row maxOccurs="unbounded" minOccurs="0" y="float">
          <column maxOccurs="unbounded" minOccurs="0" x="float">
            <shower E="float" maxOccurs="unbounded" t="float"/>
          </column>
        </row>
        <forwardShower E="float" maxOccurs="unbounded" minOccurs="0" t="float" track="int" x="float" y="float"/>
      </forwardEMcal>
    </hitView>
  </physicsEvent>

</HDDM>

<?xml version="1.0" encoding="UTF-8"?>
<hitView version="1.0">
   <centralDC minOccurs="0">
      <ring radius="float" minOccurs="0" maxOccurs="unbounded">
         <straw phim="float" maxOccurs="unbounded">
            <hit t="float" dE="float" minOccurs="0" maxOccurs="unbounded" />
            <cdcPoint z="float" r="float" phi="float" dradius="float"
                      track="int" primary="boolean" dEdx="float"
                      minOccurs="0" maxOccurs="unbounded" />
         </straw>
      </ring>
   </centralDC>

   <forwardDC minOccurs="0">
      <chamber module="int" layer="int" maxOccurs="unbounded">
         <cathodePlane z="float" tau="float" minOccurs="0" maxOccurs="unbounded">
            <strip u="float" maxOccurs="unbounded" />
         </cathodePlane>
         <anodePlane z="float" tau="float" minOccurs="0" maxOccurs="unbounded">
            <wire u="float" maxOccurs="unbounded">
               <hit t="float" dE="float" minOccurs="0" maxOccurs="unbounded" />
               <fdcPoint z="float" x="float" y="float" dradius="float"
                         track="int" primary="boolean" dEdx="float"
                         minOccurs="0" maxOccurs="unbounded" />
            </wire>
         </anodePlane>
      </chamber>
   </forwardDC>

   <startCntr minOccurs="0">
      <paddle phim="float" minOccurs="0" maxOccurs="unbounded">
         <hit t="float" dE="float" minOccurs="0" maxOccurs="unbounded" />
      </paddle>
      <startPoint t="float" z="float" r="float" phi="float"
                  track="int" primary="boolean" dEdx="float"
                  minOccurs="0" maxOccurs="unbounded" />
   </startCntr>

   <barrelEMcal minOccurs="0">
     <mod module ="int" minOccurs="0" maxOccurs="unbounded">  
      <shell  layer="int" minOccurs="0" maxOccurs="9">
       <cone  sector="int" minOccurs="0" maxOccurs="4">
         <upstream minOccurs="0">
            <shower t="float" E="float" maxOccurs="unbounded" />
         </upstream>
         <downstream minOccurs="0">
            <shower t="float" E="float" maxOccurs="unbounded" />
         </downstream> 
       </cone>
      </shell>
     </mod>

      <barrelShower t="float" z="float" phi="float" r="float"
                    E="float" track="int" primary="boolean"
                    minOccurs="0" maxOccurs="unbounded" />
   </barrelEMcal>
      
   <Cerenkov minOccurs="0">
      <section phim="float" maxOccurs="unbounded">
         <flash t="float" pe="float" maxOccurs="unbounded" />
      </section>
      <cerenkovPoint t="float" z="float" x="float" y="float"
                     E="float" px="float" py="float" pz="float"
                     track="int" primary="boolean"
                     minOccurs="0" maxOccurs="unbounded" />
   </Cerenkov>

   <forwardTOF minOccurs="0">
      <hcounter y="float" minOccurs="0" maxOccurs="unbounded">
         <left minOccurs="0">
            <hit t="float" dE="float" maxOccurs="unbounded" />
         </left>
         <right minOccurs="0">
            <hit t="float" dE="float" maxOccurs="unbounded" />
         </right>
      </hcounter>
      <vcounter x="float" minOccurs="0" maxOccurs="unbounded">
         <top minOccurs="0">
            <hit t="float" dE="float" maxOccurs="unbounded" />
         </top>
         <bottom minOccurs="0">
            <hit t="float" dE="float" maxOccurs="unbounded" />
         </bottom>
      </vcounter>
      <tofPoint t="float" x="float" y="float" z="float"
                track="int" primary="boolean"
                minOccurs="0" maxOccurs="unbounded" />
   </forwardTOF>

   <forwardEMcal minOccurs="0">
      <row y="float" minOccurs="0" maxOccurs="unbounded">
         <column x="float" minOccurs="0" maxOccurs="unbounded">
            <shower t="float" E="float" maxOccurs="unbounded" />
         </column>
      </row>
      <forwardShower t="float" x="float" y="float" z="float"
                     E="float" track="int" primary="boolean"
                     minOccurs="0" maxOccurs="unbounded" />
   </forwardEMcal>

   <upstreamEMveto minOccurs="0">
      <upvRow y="float" minOccurs="0" maxOccurs="unbounded">
         <upvLeft minOccurs="0">
            <shower t="float" E="float" maxOccurs="unbounded" />
         </upvLeft>
         <upvRight minOccurs="0">
            <shower t="float" E="float" maxOccurs="unbounded" />
         </upvRight>
      </upvRow>	 
      <upvPaddle y="float" z="float" minOccurs="0" maxOccurs="unbounded" >
         <upvLeft minOccurs="0">
            <shower t="float" E="float" maxOccurs="unbounded" />
         </upvLeft>
         <upvRight minOccurs="0">
	    <shower t="float" E="float" maxOccurs="unbounded" />
         </upvRight>
      </upvPaddle>
      <upvShower t="float" x="float" y="float" z="float" 
                 E="float" track="int" primary="boolean"
                 minOccurs="0" maxOccurs="unbounded" />
   </upstreamEMveto>

</hitView>

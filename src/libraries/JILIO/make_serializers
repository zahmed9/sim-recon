#!/bin/tcsh

rm -f hd_classes.h
touch hd_classes.h
foreach s (`cat classes | grep -v DetectorSystem_t`)
	echo "#include <$s.h>" >> hd_classes.h ;
end
foreach s (`cat classes | grep -v DetectorSystem_t`)
	echo "class $s;" >> hd_classes.h ;
end

${JILROOT}/scripts/jil_exe2xml -classes=classes `which hd_ana_d`
${JILROOT}/scripts/jil_xml2cpp hd_ana_d.xml h=hd_classes.h o=hd_serializers

mv hd_classes.h hd_serializers.h ../include

